{
  "name": "node-bloodboiler",
  "version": "2.1.0",
  "description": "A API built on top of expressJS",
  "main": "app.js",
  "author": "Lucas A Pelegrino <lucas.assuncao.p@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/lucas-a-pelegrino/node-bloodboiler.git",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "start": "NODE_ENV=production node app.js",
    "start:staging": "NODE_ENV=staging node app.js",
    "start:dev": "NODE_ENV=development nodemon app.js",
    "start:debug": "NODE_ENV=development node --inspect-brk=5858 app.js",
    "test": "jest -i",
    "test:coverage": "jest -i && codecov"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm test"
    }
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.9.25",
    "morgan": "^1.10.0",
    "nodemailer": "^6.4.6",
    "swagger-ui-express": "^4.1.4",
    "winston": "^3.2.1",
    "xss-clean": "^0.1.1",
    "yup": "^0.28.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "codecov": "^3.6.5",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-security": "^1.4.0",
    "faker": "^4.1.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "node-mocks-http": "^1.8.1",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "supertest": "^4.0.2"
  },
  "keywords": [
    "node",
    "javascript",
    "express",
    "boilerplate",
    "mongoose",
    "jsonwebtoken",
    "RESTful",
    "rest",
    "api",
    "jest"
  ]
}
